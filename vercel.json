{
  "version": 2,
  "name": "Wasteapp v2",
  "builds": [{ 
    "src": "./index.js", 
    "use": "@vercel/node",
    "config": { 
      "includeFiles": ["services/**/*", "routes/**/*", "utils/**/*"],
      "nodeVersion": "16.x"
    }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
